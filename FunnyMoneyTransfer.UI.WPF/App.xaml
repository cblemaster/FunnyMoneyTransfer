<Application x:Class="FunnyMoneyTransfer.UI.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:FunnyMoneyTransfer.UI.WPF"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!--https://stackoverflow.com/questions/22114260/how-to-use-styles-defined-in-resourcedictionary-->
                <ResourceDictionary Source="pack://application:,,,/FunnyMoneyTransfer.UI.WPF;component/User/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
